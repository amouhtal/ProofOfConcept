{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app(.*)$": "<rootDir>/src/app/$1",
    "^@Services(.*)$": "<rootDir>/src/services/$1",
    "^@Controllers(.*)$": "<rootDir>/src/controllers/$1",
    "^@Data(.*)$": "<rootDir>/src/data/$1"

  }
}
